Proceso atleta
	
	//Pedir datos
	Escribir "Ingrese tiempos realizados en correr la ruta estimado en minutos:";
	Escribir "Lunes: "; Leer Lunes;
	Escribir "Martes: "; Leer Martes;
	Escribir "Miercoles: "; Leer Miercoles;
	Escribir "Jueves: "; Leer Jueves;
	Escribir "Viernes: "; Leer Viernes;
	Escribir "Sabado: "; Leer Sabado;
	
		//Operaciones
			suma_tiempos<-Lunes+Martes+Miercoles+Jueves+Viernes+Sabado;
			prom_tiempos<-(suma_tiempos/6);
			
			
				//Mostrar resultados
					Escribir "El tiempo promedio en recorrer la ruta esta semana fue de ", prom_tiempos, " min.";
FinProceso
