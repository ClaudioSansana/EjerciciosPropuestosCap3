Proceso salario_incremento
	
	//Pedir Datos
	Escribir "Ingrese salario anterior del empleado: ";
	Leer Salario_normal;
	
		//Operaciones
			Salario_mas_aumento<-Salario_normal+(Salario_normal*0.10);
			
				//Salida
					Escribir "El salario mas el aumento del 10% recibido por el empleado da un total de: $", Salario_mas_aumento, " como saldo a recibir del empleado.";
	
FinProceso
